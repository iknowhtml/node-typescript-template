{
  "name": "node-template",
  "description": "A basic template for Node applications with linting and Babel transpilation",
  "version": "1.0.0",
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "lib/app.js",
  "scripts": {
    "start": "nodemon src/app.ts --exec ts-node",
    "build": "tsc",
    "serve": "node dist/app.js",
    "test": "mocha --require ts-node/register test/**/*.ts",
    "lint": "eslint --fix src/ test/"
  },
  "repository": "https://github.com/iknowhtml/node-template.git",
  "author": "Aki Gao",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "eslint": "^6.0.1",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {}
}
